{
  "rewrites": [
    {
      "source": "/",
      "destination": "/app/pages/index.html"
    },
    {
      "source": "/login",
      "destination": "/app/pages/login.html"
    },
    {
      "source": "/help",
      "destination": "/app/pages/help.html"
    },
    {
      "source": "/coming-soon",
      "destination": "/app/pages/coming-soon.html"
    },
    {
      "source": "/survey-pdf",
      "destination": "/app/pages/survey-pdf.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options", 
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}